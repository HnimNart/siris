x, res;
read(x);
call Fact(x, res);
print(res);

procedure Fact(n, retval)
  retval += 1;
  local i = n;
  n -= i;
  repeat
    local tmp = retval * i;
         retval <=> tmp;
    delocal tmp = retval / i;
    i -= 1;
  until (n == i || retval == 1);
  delocal i = n;
end

