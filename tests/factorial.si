procedure Fact(n, res)
    tmp;
    tmp += res * (n-1);
    if (n = 1) then {
      tmp -= res * (n-1);
      n -= 1;
    } else {
      res += tmp;
      n -= 1;
      call Fact(n , res)
    } fi (n = 0)
end


