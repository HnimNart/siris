procedure Fact(n, res)
    tmp;
    tmp += res * (n-1);
    if (n = 1) then {
      tmp -= res * (n-1);
      n -= 1;
    } else {
      res += tmp;
      n -= 1;
      call Fact(n , res)
    } fi (tmp = 0);
end

procedure Fact2(n, res)
   tmp;
   tmp -= res;
   if (tmp = res) then {
   } else {
     uncall Fact2(n, res);
     res -= n * tmp;
     n += 1;
     tmp -= res
   } fi (n = 1);
   tmp += res
end
