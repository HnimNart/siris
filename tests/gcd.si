// gg is the number of iterations

procedure GCD(a, b)
    local i = 0;
    repeat {
      i += 1;
      local t = a % b;
      a <=> b;
      b <=> t;
      delocal t = a * i % b ;
    } until (b == 0 || i == 0);
    b += i;
    delocal i = b;
end
