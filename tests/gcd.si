procedure GCD(a, b, c)
    local gcd   = a;
    local btmp  = b;
    repeat
        local t = gcd % btmp;
        gcd <=> btmp;
        btmp <=> t;
        delocal t = btmp;
    until (btmp == 0 || gcd == a);
    c += gcd;
    b += btmp;
    delocal gcd = c;
    delocal btmp = 0;
end

